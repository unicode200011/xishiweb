<mapper namespace="com.stylefeng.guns.persistence.dao.AgentWalletMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.persistence.model.AgentWallet">
        <id column="id" property="id" />
        <result column="gb_amount" property="gbAmount" />
        <result column="total_gift_amount" property="totalGiftAmount" />
        <result column="agent_id" property="agentId" />
